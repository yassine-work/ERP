@model ERP.Models.AdvantageType

@{
    ViewData["Title"] = "Advantage Type Details";
}

<div class="container mt-4">
    <h2>Advantage Type Details</h2>

    <div class="card mt-3">
        <div class="card-body">
            <h5 class="card-title">@Model.AdvantageTypeName</h5>
        </div>
    </div>

    <div class="mt-3">
        <a asp-action="Edit" asp-route-id="@Model.AdvantageTypeId" class="btn btn-warning">Edit</a>
        <a asp-action="Index" class="btn btn-secondary">Back</a>
    </div>
</div>
